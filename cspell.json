{
  "version": "0.2",
  "language": "en",
  "useGitignore": true,
  "cache": {
    "cacheLocation": ".cspellcache"
  },
  "dictionaries": [
    "en-gb",
    "lorem-ipsum",
    "shellscript",
    "npm",
    "cpp",
    "cpp-refined",
    "fullstack",
    "html",
    "css",
    "node",
    "people-names"
  ],
  "ignoreRegExpList": [
    "/@[a-zA-Z0-9-_]{4,}/g", // Username
    "/%([2-7][0-9A-Fa-f])/" // Encoded URL string
  ],
  "ignorePaths": ["src/vs", "vendor", "**/*.svg"],
  "words": [
    // Emacs terms
    "paredit",
    "minibuffer",
    "reindent",
    "Bksp",
    "dabbrev-expand",
    // npm
    "unproxify",
    "jsep",
    // VSCode API
    "webvieweditor",
    "treesticky",
    "reindentselectedlines",
    "editoridentity",
    // Names
    "Yuichiro",
    "Tachibana",
    "Tsuchiya",
    "tuttieee",
    "haruhi-s.sexp",
    "Zaha",
    "hiro-sun",
    "rkwan94",
    "NotKyon",
    "tshino",
    "Bodil",
    "Stokke",
    "bodil.file-browser",
    // Documentation
    "vendorized",
    "screencasts",
    "yanker",
    "hoge",
    "fuga",
    "piyo",
    "abcdefghij",
    "klmnopqrst"
  ]
}
